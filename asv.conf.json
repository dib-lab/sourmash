{
    "version": 1,
    "project": "sourmash",
    "project_url": "https://github.com/dib-lab/sourmash",
    "repo": ".",
    "branches": ["latest"],
    "dvcs": "git",
    "environment_type": "virtualenv",
    "pythons": ["3.7"],
    "env_dir": ".asv/env",
    "results_dir": ".asv/results",
    "html_dir": ".asv/html",
    "build_cache_size": 8,
    "build_command": [
      "python -m pip install build",
      "python -m build --sdist --wheel --outdir {build_cache_dir} {build_dir}"
    ]
}
